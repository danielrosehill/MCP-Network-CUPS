{
  "name": "mcp-cups-network-printers",
  "version": "3.0.0",
  "description": "MCP server for network printing via CUPS. Run on a print server to give AI assistants remote access to network printers. Supports file uploads, SSE transport, and CUPS printer management.",
  "author": "Daniel Rosehill (fork), Stephen Clarke (original)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielrosehill/mcp-cups-network-printers.git"
  },
  "bugs": {
    "url": "https://github.com/danielrosehill/mcp-cups-network-printers/issues"
  },
  "homepage": "https://github.com/danielrosehill/mcp-cups-network-printers#readme",
  "keywords": [
    "mcp",
    "mcp-server",
    "model-context-protocol",
    "printer",
    "printing",
    "cups",
    "network-printer",
    "remote-printing",
    "print-server",
    "sse",
    "linux",
    "llm",
    "automation",
    "cli",
    "pdf",
    "document",
    "queue"
  ],
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-cups-network-printers": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "pnpm run build",
    "test": "pnpm run test:unit && pnpm run test:integration",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration --testTimeout=30000",
    "test:watch": "vitest tests/unit",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "crossnote": "^0.9.15",
    "execa": "^9.6.0",
    "express": "^4.21.2",
    "gray-matter": "^4.0.3",
    "he": "^1.2.0",
    "highlight.js": "^11.11.1",
    "pdf-parse": "^2.4.4",
    "yn": "^5.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/he": "^1.2.3",
    "@types/node": "^24.8.1",
    "@types/pdf-parse": "^1.1.5",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
